{
  "name": "saantau",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "bun@1.3.0",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test:coverage",
    "preview": "turbo run preview",
    "check": "turbo run check",
    "format": "prettier --write .",
    "deploy": "turbo run deploy",
    "lint": "turbo run lint",
    "machine-translate": "turbo run machine-translate",
    "outdated": "turbo run outdated",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "turbo run build && changeset publish"
  },
  "dependencies": {
    "@inlang/paraglide-js": "^2.4.0",
    "bun-types": "^1.3.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@cloudflare/workers-types": "^4.20251014.0",
    "@types/bun": "latest",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "wrangler": "^4.43.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tijptjik/saantau.git"
  },
  "author": "tijptjik <m@type.hk>",
  "license": "MIT",
  "homepage": "https://github.com/tijptjik/saantau#readme",
  "bugs": {
    "url": "https://github.com/tijptjik/saantau/issues"
  }
